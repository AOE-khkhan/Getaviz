<% @scene = @object.get_assigned_scene %>
<% content_for :sidebar do %>
  <% @object = @object.test %>
  <%= render :partial => 'tests/conduct' %>
<% end %>
<% content_for :sidebar_title do %>Aufgabe<% end %>
<% content_for :fullscreen do %>
<% src = full_scene_path(@scene) %>
<% src = @scene.url if @scene.use_url %>
<iframe id="scene-iframe" src="<%= src %>" frameborder="0" style="overflow:hidden;overflow-x:hidden;overflow-y:hidden;position:absolute;top:60px;left:0px;right:0px;bottom:0px" height="1020" width="1920"></iframe>
<% end %>


